{
  "manifest_version": 3,
  "name": "Dogecoin Profit Calculator",
  "version": "1.0",
  "description": "Extension để tính toán profit từ giá Dogecoin tự động",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "scripting",
    "alarms",
    "browsingData"
  ],
  "host_permissions": [
    "https://www.mining-dutch.nl/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Dogecoin Profit Calculator"
  },
  "web_accessible_resources": [
    {
      "resources": ["test-background.html", "test-background.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}